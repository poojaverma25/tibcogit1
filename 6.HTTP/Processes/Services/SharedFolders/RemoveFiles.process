<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/RemoveFilesRequest" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/RemoveFilesResponse" xmlns:ns02="www.tibco.com/plugin/Sleep" xmlns:ns="http://www.tibco.com/schemas/0.ProjectZero/Resources/Schemas/Commons/Header" xmlns:ns0="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns3="http://www.tibco.com/schemas/0.ProjectZero/Resources/Schemas/Commons/Response">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/RemoveFilesRequest" schemaLocation="/Resources/Schemas/Services/RemoveFilesRequest.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/RemoveFilesResponse" schemaLocation="/Resources/Schemas/Services/RemoveFilesResponse.xsd"/>
    <pd:name>Processes/Services/SharedFolders/RemoveFiles.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:RemoveFilesRequest"/>
    <pd:startX>254</pd:startX>
    <pd:startY>165</pd:startY>
    <pd:returnBindings>
        <pfx2:RemoveFilesResponse>
            <xsl:copy-of select="$Start/pfx:RemoveFilesRequest/ns:Header"/>
            <pfx2:Body>
                <ns3:Response>
                    <ns3:message>
                        <xsl:value-of select="'file has been removed'"/>
                    </ns3:message>
                    <ns3:responseStatus>
                        <xsl:value-of select="'SUCCESS'"/>
                    </ns3:responseStatus>
                    <ns3:data>
                        <xsl:copy-of select="$Remove-File/ns2:RemoveActivityOutputClass/fileInfo"/>
                    </ns3:data>
                </ns3:Response>
            </pfx2:Body>
        </pfx2:RemoveFilesResponse>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:RemoveFilesResponse"/>
    <pd:endX>733</pd:endX>
    <pd:endY>162</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1669803264246</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>344</pd:x>
        <pd:y>25</pd:y>
        <pd:width>306</pd:width>
        <pd:height>262</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:errorCondition>$index = 3</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Remove File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Remove File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Remove File</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-65536</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Remove File">
            <pd:type>com.tibco.plugin.file.FileRemoveActivity</pd:type>
            <pd:resourceType>ae.activities.FileRemoveActivity</pd:resourceType>
            <pd:x>440</pd:x>
            <pd:y>97</pd:y>
            <config/>
            <pd:inputBindings>
                <ns2:RemoveActivityInputClass>
                    <fileName>
                        <xsl:value-of select="concat($Start/pfx:RemoveFilesRequest/pfx:Body/pfx:source/pfx:sourceDirectoryPath,$Start/pfx:RemoveFilesRequest/pfx:Body/pfx:source/pfx:sourceFileName)"/>
                    </fileName>
                </ns2:RemoveActivityInputClass>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>444</pd:x>
            <pd:y>221</pd:y>
            <config/>
            <pd:inputBindings>
                <ns02:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$index* 10000"/>
                    </IntervalInMillisec>
                </ns02:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>585</pd:x>
            <pd:y>221</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns0:ActivityInput>
                    <message>
                        <xsl:value-of select="concat('File has not been removed. Retry attempt.',$index)"/>
                    </message>
                </ns0:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>